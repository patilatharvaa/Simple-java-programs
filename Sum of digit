import java.util.*;
// find the sum of even or odd no
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("enter the no: ");
        int n = sc.nextInt();
        int sum = 0;
        for(;n>0;){
            int rem = n%10;
            sum += rem;
            n /= 10;
        }
        System.out.print(sum);
    }
}
