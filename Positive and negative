import java.util.*;
// find the sum of even or odd no
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("enter the no : ");
        int n = sc.nextInt();
        if (n > 0) {
            System.out.println("Positive");
        }else{
            System.out.println("Negative");
        }
    }
}
