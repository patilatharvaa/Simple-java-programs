import java.util.*;
// find the sum of even or odd no
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("enter the no1 : ");
        int no1 = sc.nextInt();
        int no2 = sc.nextInt();
        int no3 = sc.nextInt();
        if (no1 > no2 &&  no1 > no3 ) {
            System.out.println("no1 is largest no");
        }else if (no2 > no3) {
            System.out.println("no2 is largest no");
        }else{
            System.out.println("no3 is largest no");
        }
    }
}
